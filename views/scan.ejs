	<script>


		function createPro(pro){
			var progress = Math.round(pro.currentMoney/pro.goalMoney*100)+ "%";
			var pnode = document.querySelectorAll('.projectItem')[0].cloneNode(true);
			var imgsrc ="images\/" + pro.img[0];
			pnode.querySelectorAll("img")[0].src = imgsrc;
			pnode.querySelectorAll(".title")[0].innerHTML = pro.proName;
			pnode.querySelectorAll(".J_progress")[0].innerHTML = progress;
			pnode.querySelectorAll(".J_curmoney")[0].innerHTML = pro.currentMoney;
			pnode.querySelectorAll(".J_support")[0].innerHTML = pro.supportNum;
			var returnNode = document.getElementById('J_Projects').appendChild(pnode);
		}

		function init(){
			$.ajax({
				type: "get",
				url: "/getprolist",
				success: function(data) {
					project = data;
					var proNum = data.length;
					for(var i = 0;i<proNum;i++){
						createPro(data[i]);
					}
					goDetail(data);
				},
				error: function(data) {
					alert("失败");
				}
			})
		}

		function goDetail(data){
			$("#J_Projects li").click(function(){
				index = $(this).index();
				project =data[index-1];
				window.open("detail").focus();
			})
		}
		init();
	</script>

