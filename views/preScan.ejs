                        <div class="nav-son" id="J_navSon" style="display: none;">
                            <span class="triangle"></span>
                            <ul class="J_navallType category-drop-down">
                                <li class="first">
                                    <a class="category-121278001" href="scan" target="_blank">影音</a>
                                </li>
                                <li>
                                    <a class="category-121280001" href="scan" target="_blank">公益</a>
                                </li>
                                <li>
                                    <a class="category-121274002" href="scan" target="_blank">书籍</a>
                                </li>
                                <li>
