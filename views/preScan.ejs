                    <li class="publish">
                        <a class="link" href="startPro" target="_blank">发起项目</a>
                    </li>
                </ul>
            </nav>
        </header>
        <h1>我的项目</h1>
        <div class="wrap">
            <h2>项目信息</h2>
            <p ><span class="proTit">项目类型</span><span id="proType"></span></p>
            <p ><span class="proTit">项目名称</span><span id="proName"></span></p>
            <p ><span class="proTit">项目简介</span><span id="proTitle"></span></p>
            <p ><span class="proTit">筹资金额</span><span id="goalMoney"></span></p>
            <p ><span class="proTit">筹资天数</span><span id="during"></span></p>
            <p><span class="proTit">项目说明</span><span id="proDescribe"></span></p>
            <div class="proImg"><span class="proTit">项目图片</span></div>
            <div id="proImg"></div>
            <h2>回报设置</h2>
            <div id="perSupport">
                <p><span class="proTit">10元</span><span id="payBack1"></span></p>
                <p><span class="proTit">100元</span><span id="payBack2"></span></p>
                <p><span class="proTit">200元</span><span id="payBack3"></span></p>
            </div>
            <div><span class="proT proTit">发起时间</span><span id="startTime"></span></div>
            <div><span class="proT proTit">结束时间</span><span id="finishTime"></span></div>

        </div>

    </div>


    <script>

       $(document).ready(function(){

          var Domen = {};

          function changeDom(domen){
              $("#proType").html(domen.proType);
              $("#proName").html(domen.proName);
              $("#proTitle").html(domen.proTitle);
              $("#goalMoney").html(domen.goalMoney);
              $("#proDescribe").html(domen.proDescribe);
              $("#during").html(domen.during);
              $("#proImg").html("<img src=\"/images/"+domen.img[0]+"\">");
              $("#payBack1").html(domen.perSupport.payBack1);
              $("#payBack2").html(domen.perSupport.payBack2);
              $("#payBack3").html(domen.perSupport.payBack3);
              $("#startTime").html(domen.startTime.substr(0,10));
              $("#finishTime").html(domen.finishTime.substr(0,10));
          }

          $.ajax({
              type:"get",
              url:"/getprolist",
              success:function(data){
                  var n= data.length;
                  Domen = data[n-1];
                  changeDom(Domen);
              },
              error:function(data){
                  alert("failed");
              }
          })

           //导航
           var scan = $(".J_list");
           var scanNavSon = $("#J_navSon");
           var myPro = $(".myPro");
           var proNavSon = $("#pro-nav-son");
           scan.click(function(){
               scanNavSon.slideToggle();
           });
           myPro.click(function(){
               proNavSon.slideToggle();
           });

      });




    </script>


</body>
</html>